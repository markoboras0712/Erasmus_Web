\n          <v-text-field\r\n            :label=\"$t('password')\"\r\n            required\r\n            :rules=\"required\"\r\n            v-model=\"password\"\r\n            :type=\"showPassword ? 'text' : 'password'\"\r\n            prepend-icon=\"mdi-lock\"\r\n            :append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\"\r\n            @click:append=\"showPassword = !showPassword\"\r\n          />\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-divider></v-divider>\r\n      <v-card-actions>\r\n        <v-btn color=\"success\" to=\"/signup\">{{ $t('signup') }}</v-btn>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"info\" @click=\"onSubmit\" :disabled=\"!valid\">{{ $t('login') }}</v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport messages from '../../data/authRules'\r\nimport beaches from '../../data/novalja/beaches.js'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      valid: true,\r\n      showPassword: false,\r\n      email: '',\r\n      password: ''\r\n    }\r\n  },\r\n  methods: {\r\n    onSubmit () {\r\n      const formData = {\r\n        email: this.email,\r\n        password: this.password\r\n      }\r\n      this.$store.dispatch('login', {\r\n        email: formData.email,\r\n        password: formData.password\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    required () {\r\n      return [v => !!v || this.$t('req')]\r\n    },\r\n    beaches () {\r\n      return beaches\r\n    }\r\n  },\r\n  i18n: {\r\n    messages\r\n  }\r\n}\r\n</script>\r\n"]}]}