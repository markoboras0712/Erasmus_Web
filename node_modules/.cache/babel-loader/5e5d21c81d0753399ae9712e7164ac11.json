ata.de.title\"\r\n          :counter=\"10\"\r\n          :rules=\"titleRules\"\r\n          :label=\"'de - ' + $t('title.title')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.it.title\"\r\n          :counter=\"10\"\r\n          :rules=\"titleRules\"\r\n          :label=\"'it - ' + $t('title.title')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.sl.title\"\r\n          :counter=\"10\"\r\n          :rules=\"titleRules\"\r\n          :label=\"'sl - ' + $t('title.title')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.cs.title\"\r\n          :counter=\"10\"\r\n          :rules=\"titleRules\"\r\n          :label=\"'cs - ' + $t('title.title')\"\r\n          required\r\n        ></v-text-field>\r\n      </div>\r\n\r\n      <v-divider class=\"mb-3 mt-3\"></v-divider>\r\n\r\n      <v-btn @click=\"showTypeForm = !showTypeForm\">{{ $t('type.type') }}:</v-btn>\r\n      <div v-show=\"showTypeForm\">\r\n        <v-text-field\r\n          v-model=\"formData.hr.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'hr - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.en.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'en - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.de.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'de - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.it.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'it - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.sl.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'sl - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.cs.type\"\r\n          :rules=\"typeRules\"\r\n          :label=\"'cs - ' + $t('type.type')\"\r\n          required\r\n        ></v-text-field>\r\n      </div>\r\n\r\n      <v-divider class=\"mb-3 mt-3\"></v-divider>\r\n\r\n      <v-btn @click=\"showInfoForm = !showInfoForm\" class=\"mb-3\">{{ $t('info.info') }}:</v-btn>\r\n      <div v-show=\"showInfoForm\">\r\n        <v-btn @click=\"infoCounter++\">{{ $t('info.addInfo') }}</v-btn>\r\n        <v-btn v-if=\"infoCounter>0\" @click=\"infoCounter--\" class=\"ml-3\">{{ $t('info.reduceInfo') }}</v-btn>\r\n        <div v-for=\"i in infoCounter\" :key=\"i\">\r\n        <h4>{{ $t('info.info') }} - {{ i }}.</h4>\r\n        <v-text-field\r\n          v-model=\"formData.hr.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'hr - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.en.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'en - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.de.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'de - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.it.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'it - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.sl.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'sl - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"formData.cs.info[i-1]\"\r\n          :rules=\"infoRules\"\r\n          :label=\"'cs - ' + $t('info.info')\"\r\n          required\r\n        ></v-text-field>\r\n        <v-divider></v-divider>\r\n        </div>\r\n      </div>\r\n\r\n      <div>{{ formData.cs.info }}</div>\r\n\r\n      <v-row justify=\"end\">\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"success\"\r\n          class=\"mr-4\"\r\n          @click=\"saveData\"\r\n        >\r\n          {{ $t('saveApartment') }}\r\n        </v-btn>\r\n\r\n        <v-btn color=\"error\" class=\"mr-4\" @click=\"reset\"> {{ $t('resetForm') }} </v-btn>\r\n\r\n      </v-row>\r\n    </v-form>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport formTranslations from '../data/formTranslations'\r\n\r\nexport default {\r\n  data: () => ({\r\n    valid: true,\r\n    showInfoForm: false,\r\n    showTitleForm: false,\r\n    showTypeForm: false,\r\n    formData: {\r\n      hr: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      },\r\n      en: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      },\r\n      de: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      },\r\n      it: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      },\r\n      sl: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      },\r\n      cs: {\r\n        title: '',\r\n        type: '',\r\n        info: []\r\n      }\r\n    },\r\n    infoCounter: 1,\r\n    title: '',\r\n    type: '',\r\n    lazy: false\r\n  }),\r\n  computed: {\r\n    titleRules () {\r\n      return [\r\n        (v) => !!v || this.$t('title.titleRequired'),\r\n        (v) => (v && v.length <= 10) || this.$t('title.titleLess')\r\n      ]\r\n    },\r\n    typeRules () {\r\n      return [(v) => !!v || this.$t('type.typeRequired')]\r\n    },\r\n    infoRules () {\r\n      return [(v) => !!v || this.$t('info.infoRequired')]\r\n    }\r\n  },\r\n  methods: {\r\n    validate () {\r\n      this.$refs.form.validate()\r\n    },\r\n    reset () {\r\n      this.$refs.form.reset()\r\n    },\r\n    saveData () {\r\n      axios.post('/apartments.json' + '?auth=' + this.$store.getters.idToken, this.formData)\r\n        .then(res => console.log(res))\r\n        .catch(err => console.log(err))\r\n    }\r\n  },\r\n  i18n: {\r\n    messages: formTranslations\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}