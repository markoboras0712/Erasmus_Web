A6BA,EAAA,IAAA,EAAA;AACA,IAAA,QAAA,EAAA;AADA;AA7BA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"mt-10\" fluid>\r\n    <v-carousel hide-delimiters style=\"height: 45vh\">\r\n      <v-carousel-item\r\n      v-for=\"(beach,i) in beaches\"\r\n      :key=\"i\"\r\n      :src=\"beach.image\"\r\n    ></v-carousel-item>\r\n        </v-carousel>\r\n    <v-card class=\"mt-15\" style=\"height: 70vh\">\r\n      <v-card-title>\r\n        <h1 class=\"display-1\">{{ $t('login')}}</h1>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\" :label=\"$t('email')\">\r\n          <v-text-field :rules=\"required\" :label=\"this.$t('email')\" prepend-icon=\"mdi-email\" v-model=\"email\" required/>\r\n          <v-text-field\r\n            :label=\"$t('password')\"\r\n            required\r\n            :rules=\"required\"\r\n            v-model=\"password\"\r\n            :type=\"showPassword ? 'text' : 'password'\"\r\n            prepend-icon=\"mdi-lock\"\r\n            :append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\"\r\n            @click:append=\"showPassword = !showPassword\"\r\n          />\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-divider></v-divider>\r\n      <v-card-actions>\r\n        <v-btn color=\"success\" to=\"/signup\">{{ $t('signup') }}</v-btn>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"info\" @click=\"onSubmit\" :disabled=\"!valid\">{{ $t('login') }}</v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport messages from '../../data/authRules'\r\nimport beaches from '../../data/novalja/beaches.js'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      valid: true,\r\n      showPassword: false,\r\n      email: '',\r\n      password: ''\r\n    }\r\n  },\r\n  methods: {\r\n    onSubmit () {\r\n      const formData = {\r\n        email: this.email,\r\n        password: this.password\r\n      }\r\n      this.$store.d