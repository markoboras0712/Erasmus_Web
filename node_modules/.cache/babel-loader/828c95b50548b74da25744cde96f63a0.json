GAAA;AACA;AATA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <v-container>\r\n      <v-card\r\n        v-for=\"(reservation, index) in reservations\"\r\n        :key=\"(reservation, index)\"\r\n        class=\"mb-4\"\r\n      >\r\n        <v-card-title>{{ apartment(reservation.apartmentId) }}</v-card-title>\r\n        <v-card-subtitle v-if=\"!isAdmin\">{{\r\n          reservation.userEmail\r\n        }}</v-card-subtitle>\r\n        <v-card-text>{{reservation.dates[0]}} - {{reservation.dates[1]}}</v-card-text>\r\n      </v-card>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport dynamicLocalization from '../mixins/dynamicLocalization'\r\nexport default {\r\n  data () {\r\n    return {\r\n      reservations: null\r\n    }\r\n  },\r\n  mixins: [dynamicLocalization],\r\n  created () {\r\n    if (this.$store.getters.user(localStorage.email)) {\r\n      if (this.$store.getters.user(localStorage.email).admin) {\r\n        console.log('Admin je')\r\n        console.log(this.$store.getters.reservations)\r\n        this.reservations = this.$store.getters.reservations\r\n      } else {\r\n        this.reservations = this.$store.getters.userReservations(\r\n          this.$store.getters.userId\r\n        )\r\n      }\r\n    }\r\n    console.log('rezervacije')\r\n    console.log(this.reservations)\r\n  },\r\n  computed: {\r\n    isAdmin () {\r\n      return this.$store.getters.user(localStorage.email).admin\r\n    },\r\n    apartment (id) {\r\n      const res = this.$store.getters.apartment(id)\r\n      console.log('apartmant')\r\n      console.log(res)\r\n      return res\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views"}]}